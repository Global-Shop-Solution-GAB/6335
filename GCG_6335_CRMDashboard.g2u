Program.Sub.LoadEntryColumns.Start
F.Intrinsic.Control.Try

V.Local.sError.Declare(String)

F.Data.DataTable.CreateFromSQL("dtEntryColumns", "Con", "select OID, IsNull(BED_COUNT, 0) BED_COUNT, IsNull(DAILY_CENSUS, 0) DAILY_CENSUS, IsNull(AVERAGE_LENGTH, 0) AVERAGE_LENGTH, IsNull(PERCENT_QUALIFIED, 0) PERCENT_QUALIFIED, IsNull(Price, 0) PRICE, IsNull(PROJECTED_STRAPS, 0) PROJECTED_STRAPS from GCG_6335_FIELDS")

F.Data.Dictionary.CreateFromDataTable("dictTemp", "dtEntryColumns", "OID", "BED_COUNT")
F.Data.Dictionary.SetDefaultReturn("dictTemp", 0)
F.Data.DataTable.AddColumn("dtOpp", "BED_COUNT", "Long", 0)
F.Data.DataTable.FillFromDictionary("dtOpp", "dictTemp", "OID", "BED_COUNT")
F.Data.Dictionary.Close("dictTemp")

F.Data.Dictionary.CreateFromDataTable("dictTemp", "dtEntryColumns", "OID", "DAILY_CENSUS")
F.Data.Dictionary.SetDefaultReturn("dictTemp", 0)
F.Data.DataTable.AddColumn("dtOpp", "DAILY_CENSUS", "Long", 0)
F.Data.DataTable.FillFromDictionary("dtOpp", "dictTemp", "OID", "DAILY_CENSUS")
F.Data.Dictionary.Close("dictTemp")

F.Data.Dictionary.CreateFromDataTable("dictTemp", "dtEntryColumns", "OID", "AVERAGE_LENGTH")
F.Data.Dictionary.SetDefaultReturn("dictTemp", 0)
F.Data.DataTable.AddColumn("dtOpp", "AVERAGE_LENGTH", "Long", 0)
F.Data.DataTable.FillFromDictionary("dtOpp", "dictTemp", "OID", "AVERAGE_LENGTH")
F.Data.Dictionary.Close("dictTemp")

F.Data.Dictionary.CreateFromDataTable("dictTemp", "dtEntryColumns", "OID", "PERCENT_QUALIFIED")
F.Data.Dictionary.SetDefaultReturn("dictTemp", 0)
F.Data.DataTable.AddColumn("dtOpp", "PERCENT_QUALIFIED", "Float", 0)
F.Data.DataTable.FillFromDictionary("dtOpp", "dictTemp", "OID", "PERCENT_QUALIFIED")
F.Data.Dictionary.Close("dictTemp")

F.Data.Dictionary.CreateFromDataTable("dictTemp", "dtEntryColumns", "OID", "PRICE")
F.Data.Dictionary.SetDefaultReturn("dictTemp", 0)
F.Data.DataTable.AddColumn("dtOpp", "PRICE", "Float", 0)
F.Data.DataTable.FillFromDictionary("dtOpp", "dictTemp", "OID", "PRICE")
F.Data.Dictionary.Close("dictTemp")

F.Data.Dictionary.CreateFromDataTable("dictTemp", "dtEntryColumns", "OID", "PROJECTED_STRAPS")
F.Data.Dictionary.SetDefaultReturn("dictTemp", 0)
F.Data.DataTable.AddColumn("dtOpp", "PROJECTED_STRAPS", "Float", 0)
F.Data.DataTable.FillFromDictionary("dtOpp", "dictTemp", "OID", "PROJECTED_STRAPS")
F.Data.Dictionary.Close("dictTemp")

F.Data.DataTable.Close("dtEntryColumns")

'now load monthly strap sales
F.Data.Dictionary.CreateFromSQL("dictTemp", "Con", "select CUSTOMER, ROUND(SUM(QTY_SHIPPED) / 12, 0) from V_ORDER_HIST_LINE where DATE_SHIPPED >= DATEADD(YEAR, -1, CURDATE()) and PART in (select PART from V_INVENTORY_MSTR where CODE_SORT = 'STRAP') group by CUSTOMER")
F.Data.Dictionary.SetDefaultReturn("dictTemp", 0)
F.Data.DataTable.AddColumn("dtOpp", "MONTHLY_STRAP_SALES", "Long", 0)
F.Data.DataTable.FillFromDictionary("dtOpp", "dictTemp", "OID", "MONTHLY_STRAP_SALES")
F.Data.Dictionary.Close("dictTemp")

F.Data.DataTable.AddExpressionColumn("dtOpp", "ANNUAL_QUALIFIED_PATIENTS", "Float", "IIF(IsNull([AVERAGE_LENGTH], 0) <> 0, ([DAILY_CENSUS] * ([PERCENT_QUALIFIED] / 100) * 365) / [AVERAGE_LENGTH], 0)")
F.Data.DataTable.AddExpressionColumn("dtOpp", "MONTHLY_PROJECTED_STRAPS_CALCULATED", "Float", "([ANNUAL_QUALIFIED_PATIENTS] * 2) / 12")
F.Data.DataTable.AddExpressionColumn("dtOpp", "FINAL_PROJECTED_STRAPS", "Float", "IIF(IsNull([PROJECTED_STRAPS], 0) = 0, [MONTHLY_PROJECTED_STRAPS_CALCULATED], [PROJECTED_STRAPS])")
F.Data.DataTable.AddExpressionColumn("dtOpp", "EXPECTED_REVENUE", "Float", "([FINAL_PROJECTED_STRAPS] * [PRICE])")
F.Data.DataTable.AddExpressionColumn("dtOpp", "MONTHLY_PROJECTED_CONTROLLERS", "Float", "([BED_COUNT] * 2) / 12")
F.Data.DataTable.AddExpressionColumn("dtOpp", "MONTHLY_PROJECTED_CHARGERS", "Float", "[BED_COUNT]/ 12")

F.Intrinsic.Control.Catch
	F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.End
F.Intrinsic.Control.EndTry
Program.Sub.LoadEntryColumns.End

Program.Sub.LoadCharts.Start
F.Intrinsic.Control.Try

V.Local.sError.Declare(String)
V.Local.sFilter.Declare(String)
V.Local.sPieChartData.Declare(String)
V.Local.iCounter.Declare(Long)
V.Local.iStraps.Declare(Long)
V.Local.iControllers.Declare(Long)
V.Local.iChargers.Declare(Long)
V.Local.sRevenueChartData.Declare(String, "None*!*0")
V.Local.sFormat.Declare(String)
V.Local.sFormat2.Declare(String)
V.Local.sFormat3.Declare(String)
V.Local.sVolumeDataEstimated.Declare(String, "None*!*0")
V.Local.sVolumeDataActual.Declare(String, "None*!*0")

F.Data.DataTable.AddExpressionColumn("dtOpp", "ANNUAL_QUALIFIED_PATIENTS2", "Float", "IIF(IsNull([AVERAGE_LENGTH], 0) <> 0, ([DAILY_CENSUS] * ([PERCENT_QUALIFIED] / 100) * 365) / [AVERAGE_LENGTH], 0)")
F.Data.DataTable.AddExpressionColumn("dtOpp", "MONTHLY_PROJECTED_STRAPS_CALCULATED2", "Float", "([ANNUAL_QUALIFIED_PATIENTS2] * 2) / 12")
F.Data.DataTable.AddExpressionColumn("dtOpp", "FINAL_PROJECTED_STRAPS2", "Float", "IIF(IsNull([PROJECTED_STRAPS], 0) = 0, [MONTHLY_PROJECTED_STRAPS_CALCULATED2], [PROJECTED_STRAPS])")
F.Data.DataTable.AddExpressionColumn("dtOpp", "EXPECTED_REVENUE2", "Float", "([FINAL_PROJECTED_STRAPS2] * [PRICE])")
F.Data.DataTable.AddExpressionColumn("dtOpp", "MONTHLY_PROJECTED_CONTROLLERS2", "Float", "([BED_COUNT] * 2) / 12")
F.Data.DataTable.AddExpressionColumn("dtOpp", "MONTHLY_PROJECTED_CHARGERS2", "Float", "[BED_COUNT]/ 12")

GUI.frmCRM.GsGCOpp.FilterToWhereClause("gvOpp", V.Local.sFilter)
F.Data.DataView.Create("dtOpp", "dvOpp", 22, V.Local.sFilter, "")
F.Intrinsic.Control.For(V.Local.iCounter, 0, V.DataView.dtOpp!dvOpp.RowCount--, 1)
	F.Intrinsic.Control.If(V.Local.iCounter, =, 0)
		F.Intrinsic.String.Format(V.DataView.dtOpp!dvOpp(V.Local.iCounter).EXPECTED_REVENUE2!FieldValFloat, "0.00", V.Local.sFormat)
		F.Intrinsic.String.Build("{0}*!*{1}", V.DataView.dtOpp!dvOpp(V.Local.iCounter).COMPID!FieldValTrim, V.Local.sFormat, V.Local.sRevenueChartData)
		F.Intrinsic.String.Build("{0}*!*{1}", V.DataView.dtOpp!dvOpp(V.Local.iCounter).COMPID!FieldValTrim, V.DataView.dtOpp!dvOpp(V.Local.iCounter).MONTHLY_STRAP_SALES!FieldValLong, V.Local.sVolumeDataActual)
		F.Intrinsic.String.Build("{0}*!*{1}", V.DataView.dtOpp!dvOpp(V.Local.iCounter).COMPID!FieldValTrim, V.DataView.dtOpp!dvOpp(V.Local.iCounter).FINAL_PROJECTED_STRAPS2!FieldValLong, V.Local.sVolumeDataEstimated)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Format(V.DataView.dtOpp!dvOpp(V.Local.iCounter).EXPECTED_REVENUE2!FieldValFloat, "0.00", V.Local.sFormat)
		F.Intrinsic.String.Build("{2}!*!{0}*!*{1}", V.DataView.dtOpp!dvOpp(V.Local.iCounter).COMPID!FieldValTrim, V.Local.sFormat, V.Local.sRevenueChartData, V.Local.sRevenueChartData)
		F.Intrinsic.String.Build("{2}!*!{0}*!*{1}", V.DataView.dtOpp!dvOpp(V.Local.iCounter).COMPID!FieldValTrim, V.DataView.dtOpp!dvOpp(V.Local.iCounter).MONTHLY_STRAP_SALES!FieldValLong, V.Local.sVolumeDataActual, V.Local.sVolumeDataActual)
		F.Intrinsic.String.Build("{2}!*!{0}*!*{1}", V.DataView.dtOpp!dvOpp(V.Local.iCounter).COMPID!FieldValTrim, V.DataView.dtOpp!dvOpp(V.Local.iCounter).FINAL_PROJECTED_STRAPS2!FieldValLong, V.Local.sVolumeDataEstimated, V.Local.sVolumeDataEstimated)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.Next(V.Local.iCounter)
F.Data.DataView.Close("dtOpp", "dvOpp")

'load pie chart
F.Data.DataTable.Compute("dtOpp", "SUM(FINAL_PROJECTED_STRAPS2)", V.Local.sFilter, V.Local.iStraps)
