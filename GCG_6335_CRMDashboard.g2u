	'Create DataTable
	F.Intrinsic.Control.If(V.Global.bIsCRMsupervisor)
		'IF CRM SUPERVISOR SHOW ALL		
		V.Local.sSQL.Set("SELECT CM.OID, CM.COMPID,CASE WHEN COMPTYPE = 10 THEN VVM.NAME_VENDOR WHEN COMPTYPE = 15 THEN VCM.NAME_CUSTOMER WHEN COMPTYPE = 18 THEN PM.NAME_PROSPECT WHEN COMPTYPE = 19 THEN PM.NAME_PROSPECT END,CASE WHEN CM.COMPTYPE = 10 THEN 'V' WHEN CM.COMPTYPE = 15 THEN 'C' WHEN CM.COMPTYPE = 18 THEN 'P' WHEN CM.COMPTYPE = 19 THEN 'S' END, CM.ODESC, CTYPE.VALUE, COG.OGDESC, CT.NAME, CM.SALESORDER, CM.QUOTE, SP.NAME, CM.INT_ASSIGN, CSTAT.VALUE, COF.VALUE, COP.VALUE, COL.VALUE, CM.OCREATEDATE, CM.OPROJCLOSEDATE, CM.OCLOSEDATE, CR.VALUE AS CLOSEREASON, CM.OREVENUE, COMPTYPE FROM CRM_OPP_MASTER CM LEFT JOIN V_CUSTOMER_MASTER VCM ON CM.COMPID = VCM.CUSTOMER LEFT JOIN PROSPECT_MASTER PM ON CM.COMPID = PM.PROSPECT LEFT JOIN V_VENDOR_MASTER VVM ON CM.COMPID = VVM.VENDOR LEFT JOIN CRM_OPP_TYPE CTYPE ON CM.OTYPE = CTYPE.SEQ LEFT JOIN CRM_OPPGROUPING COG ON CM.OGID = COG.OGID LEFT JOIN CRM_OPP_STATUS CSTAT ON CM.OSTATUS = CSTAT.SEQ LEFT JOIN CRM_OPP_FUNNEL COF ON CM.OFUNNEL = COF.SEQ LEFT JOIN CRM_OPP_PROB COP ON CM.OPROB = COP.ID AND CM.OFUNNEL = COP.FUNNELID LEFT JOIN CRM_OPP_LEADSOURCE COL ON CM.OLEADSOURCE = COL.SEQ LEFT JOIN SALESPERSONS SP ON CM.SALESPERSON = SP.ID LEFT JOIN CONTACT CT ON CM.COMPID = CT.CUST AND CM.CID = CT.ALT_ID LEFT JOIN CRM_OPP_CLSRSN CR ON CM.OCLOSEREASON = CR.SEQ ORDER BY CM.COMPID;")
	F.Intrinsic.Control.Else
		'IF NOT CRM SUPERVISOR THEN ONLY SHOW CUSTOMERS THAT USER HAS SECURITY TO SEE		
		f.Intrinsic.String.Build("SELECT CM.OID, CM.COMPID,CASE WHEN COMPTYPE = 10 THEN VVM.NAME_VENDOR WHEN COMPTYPE = 15 THEN VCM.NAME_CUSTOMER WHEN COMPTYPE = 18 THEN PM.NAME_PROSPECT WHEN COMPTYPE = 19 THEN PM.NAME_PROSPECT END,CASE WHEN CM.COMPTYPE = 10 THEN 'V' WHEN CM.COMPTYPE = 15 THEN 'C' WHEN CM.COMPTYPE = 18 THEN 'P' WHEN CM.COMPTYPE = 19 THEN 'S' END, CM.ODESC, CTYPE.VALUE, COG.OGDESC, CT.NAME, CM.SALESORDER, CM.QUOTE, SP.NAME, CM.INT_ASSIGN, CSTAT.VALUE, COF.VALUE, COP.VALUE, COL.VALUE, CM.OCREATEDATE, CM.OPROJCLOSEDATE, CM.OCLOSEDATE, CR.VALUE AS CLOSEREASON, CM.OREVENUE, COMPTYPE FROM CRM_OPP_MASTER CM LEFT JOIN V_CUSTOMER_MASTER VCM ON CM.COMPID = VCM.CUSTOMER LEFT JOIN PROSPECT_MASTER PM ON CM.COMPID = PM.PROSPECT LEFT JOIN V_VENDOR_MASTER VVM ON CM.COMPID = VVM.VENDOR LEFT JOIN CRM_OPP_TYPE CTYPE ON CM.OTYPE = CTYPE.SEQ LEFT JOIN CRM_OPPGROUPING COG ON CM.OGID = COG.OGID LEFT JOIN CRM_OPP_STATUS CSTAT ON CM.OSTATUS = CSTAT.SEQ LEFT JOIN CRM_OPP_FUNNEL COF ON CM.OFUNNEL = COF.SEQ LEFT JOIN CRM_OPP_PROB COP ON CM.OPROB = COP.ID AND CM.OFUNNEL = COP.FUNNELID LEFT JOIN CRM_OPP_LEADSOURCE COL ON CM.OLEADSOURCE = COL.SEQ LEFT JOIN SALESPERSONS SP ON CM.SALESPERSON = SP.ID LEFT JOIN CONTACT CT ON CM.COMPID = CT.CUST AND CM.CID = CT.ALT_ID LEFT JOIN CRM_OPP_CLSRSN CR ON CM.OCLOSEREASON = CR.SEQ WHERE (VCM.CUSTOMER IN (SELECT CUSTOMER FROM V_CUSTOMER_MASTER WHERE (ASSGN_USR_GRP IN ({0}) AND CRM_RES_LEV = '2') OR (ASSGN_USR_GRP = '{1}' AND CRM_RES_LEV = '1') OR (CRM_RES_LEV = '0')) OR PM.PROSPECT IN (SELECT PROSPECT FROM V_PROSPECT_MASTER WHERE (ASSGN_USR_GRP IN ({0}) AND CRM_RES_LEV = '2') OR (ASSGN_USR_GRP = '{1}' AND CRM_RES_LEV = '1') OR (CRM_RES_LEV = '0')) OR VVM.VENDOR IN (SELECT VENDOR FROM V_VENDOR_MASTER)) ORDER BY CM.COMPID;",v.Global.sUserGroups,v.Caller.User,v.Local.sSQL)
	F.Intrinsic.Control.EndIf	
		
