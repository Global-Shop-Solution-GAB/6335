Program.Sub.ResizeCharts.Start
F.Intrinsic.Control.Try
V.Local.sError.Declare
	
V.Local.iChartInterval.Declare(Long, 135)
V.Local.iGridInterval.Declare(Long, 45)
V.Local.iStartingGridSize.Declare(Long, 4950)
V.Local.iStartingChartSize.Declare(Long, 5805)
V.Local.iNewWidth.Declare(Long)
V.Local.iNewHeight.Declare(Long)
V.Local.fGridPercentage.Declare(Float)
V.Local.iTemp.Declare(Long)
V.Local.iNewChartWidth.Declare(Long)
V.Local.iNewChartHeight.Declare(Long)
V.Local.iNewChartPositionY.Declare(Long)
V.Local.iNewChartPositionX.Declare(Long)
V.Local.iNewGridHeight.Declare(Long)
V.Local.iDistanctBetweenChartAndBotton.Declare(Long, 465)

'grab new width
V.Local.iNewWidth.Set(V.Screen.frmCRM!tabCRM.Width)

'grab new height
V.Local.iNewHeight.Set(V.Screen.frmCRM!tabCRM.Height)

'calculate new chart width
F.Intrinsic.Math.Sub(V.Local.iNewWidth, 135, 135, V.Local.iNewChartWidth)
F.Intrinsic.Math.Div(V.Local.iNewChartWidth, 3, V.Local.iNewChartWidth)

'calculate new grid height
F.Intrinsic.Math.Add(V.Local.iStartingChartSize, V.Local.iStartingGridSize, V.Local.iTemp)
F.Intrinsic.Math.Div(V.Local.iStartingGridSize, V.Local.iTemp, V.Local.fGridPercentage)
F.Intrinsic.Math.Mult(V.Local.iNewHeight, V.Local.fGridPercentage, V.Local.iNewGridHeight)

'calculate new chart starting position
F.Intrinsic.Math.Add(V.Local.iNewGridHeight, V.Local.iGridInterval, V.Local.iNewChartPositionY)

'calculate new chart height
F.Intrinsic.Math.Sub(V.Local.iNewHeight, V.Local.iGridInterval, V.Local.iNewGridHeight, V.Local.iDistanctBetweenChartAndBotton, V.Local.iNewChartHeight)

GUI.frmCRM.GsGCOpp.Size(V.Screen.frmCRM!GsGCOpp.Width, V.Local.iNewGridHeight)

V.Local.iNewChartPositionX.Set(0)
GUI.frmCRM.gsChartMonthlyHardware.Position(V.Local.iNewChartPositionX, V.Local.iNewChartPositionY)
GUI.frmCRM.gsChartMonthlyHardware.Size(V.Local.iNewChartWidth, V.Local.iNewChartHeight)

F.Intrinsic.Math.Add(V.Local.iNewChartPositionX, V.Local.iNewChartWidth, V.Local.iChartInterval, V.Local.iNewChartPositionX)
GUI.frmCRM.gsChartMonthlyRevenue.Position(V.Local.iNewChartPositionX, V.Local.iNewChartPositionY)
GUI.frmCRM.gsChartMonthlyRevenue.Size(V.Local.iNewChartWidth, V.Local.iNewChartHeight)

F.Intrinsic.Math.Add(V.Local.iNewChartPositionX, V.Local.iNewChartWidth, V.Local.iChartInterval, V.Local.iNewChartPositionX)
GUI.frmCRM.gsChartMonthlyVolume.Position(V.Local.iNewChartPositionX, V.Local.iNewChartPositionY)
GUI.frmCRM.gsChartMonthlyVolume.Size(V.Local.iNewChartWidth, V.Local.iNewChartHeight)

F.Intrinsic.Control.Catch
	F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.End
F.Intrinsic.Control.EndTry
Program.Sub.ResizeCharts.End

