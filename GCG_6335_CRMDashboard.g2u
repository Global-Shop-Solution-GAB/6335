Program.Sub.CheckCRMSECURITY.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
	V.Local.sSQL.Declare	
	
	'This sub checks which user groups this user is in so that when we load the data it can filter out the ones that the user should not see based on crm user security
	'If user is CRM supervisor, it bypasses the check for crm security and shows all records.
	
	f.ODBC.Connection!cony.opencommonconnection
	
	f.Intrinsic.String.Build("SELECT A.BVAL FROM USER_OPTIONS_COM A LEFT JOIN USER_INFORMATION B ON A.USER_ID = B.USER_ID WHERE B.GS_USER = '{0}' AND A.OPTION_ID = '1010' AND A.COMPANY = '{1}';",V.Caller.User,V.Caller.CompanyCode,V.Local.sSQL)
	
	F.ODBC.Connection!CONY.EXECUTEANDRETURN(V.Local.sSQL,V.Global.bIsCRMsupervisor)
	
	f.Intrinsic.Control.If(V.Global.bIsCRMsupervisor)
		F.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	
	f.Intrinsic.String.Build("SELECT DISTINCT A.GS_GROUP FROM USER_GROUPS A LEFT JOIN MENU_SECURITY_ASSOC B ON A.GROUP_ID = B.GROUP_ID LEFT JOIN USER_INFORMATION C ON C.USER_ID = B.USER_ID WHERE C.GS_USER = '{0}';",v.Caller.User,v.Local.sSQL)
	
	f.ODBC.Connection!cony.executeandreturn(v.Local.sSQL,v.Global.sUserGroups)
	
	f.Intrinsic.String.Build("'{0}'",v.Global.sUserGroups,v.Global.sUserGroups)
	f.Intrinsic.String.Replace(v.Global.sUserGroups,"#$#","','",v.Global.sUserGroups)
	
	f.ODBC.Connection!cony.Close
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End

	F.Intrinsic.Control.EndTry
Program.Sub.CheckCRMSECURITY.End

